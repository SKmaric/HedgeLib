<?xml version="1.0" encoding="utf-8"?>
<Template>
  <IsCameraView type="bool" default="false" description="camera view" />
  <IsCollision type="bool" default="false" description="take the collision?" />
  <EaseTimeEnter type="float" default="0" description="transition time at the time of invasion (sec) ※ use only events" />
  <EaseTimeLeave type="float" default="0" description="transition time at the time of withdrawal (sec) ※ use only events" />
  <Fovy type="float" default="0" description="angle of view" />
  <ZRot type="float" default="0" description="rotation around the Z axis" />
  <Pitch type="float" default="0" description="vertical rotation" />
  <PositionMode type="enum" default="0" description="camera position mode" />
  <Distance type="float" default="0" description="distance from the target" />
  <TargetType type="enum" default="0" description="type of target" />
  <Target type="target" description="Target" />
  <TargetPositionFix type="position" description="coordinates as a target" />
  <TargetOffsetRight type="float" default="0" description="offset left and right of the target coordinate system" />
  <TargetOffsetUp type="float" default="0" description="offset the top and bottom of the target coordinate system" />
  <TargetOffsetFront type="float" default="0" description="before and after the offset of the target coordinate system" />
  <OffsetYaw type="float" default="0" description="gazing point offset (Yaw)" />
  <OffsetPitch type="float" default="0" description="gazing point offset (Pitch)" />
  <Extra type="RawByteLength" length="72" />
</Template>