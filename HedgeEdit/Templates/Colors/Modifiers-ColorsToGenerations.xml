<?xml version="1.0" encoding="utf-8"?>
<!-- Move to Templates/Colors directory in Sonic Colors Set Editor directory.-->
<Modifiers>
	<Rename>
		<!-- Element is Generations name, Value is Colors name.-->
		<!-- Common -->
		<SpringFake Value="DummySpring" />
		<DashPanel Value="GrindBooster" Condition="IsVisibleModel:false" />
		<GrindDashPanel Value="GrindBooster" Condition="IsVisibleModel:true" />
		<SelectCanon Value="JumpSelector" />	<!-- Comment out for Unleashed -->
		<JumpPole Value="SpringPole" />
		<JumpBoard3D Value="JumpBoard" Condition="SizeType>0" />
		<AdlibTrickJump Value="TrickJump" />
		<MovingThorn Value="MovingThornBall" />
		<Item Value="1UP" />
		<SshBubble Value="BubbleGenerator" />
		<Fan Value="WaterFan" />
		<RedMedal Value="RedRing" />
		<ObjectPhysics Value="Gismo" />
		<ObjectPhysics Value="MapPartsMesh" />
		<ObjectPhysics Value="MapPartsBox" />
		<GeneralFloor Value="CommonFloor" />
		<PlaTramCar Value="RollerCoaster" />
		<ThornCylinder2M Value="ThornCylinder" Condition="IsLongModel:false" />
		<ThornCylinder3M Value="ThornCylinder" Condition="IsLongModel:true" />
		<!-- System -->
		<LayerChangeCollisionFlat Value="LayerBoard" />
		<ObjFlowCollision Value="FlowEx" />
		<EventCollision Value="SwitchCollision" />
		<!-- Player System -->
		<AutorunFinishCollision Value="AutorunCollisionEnd" />
		<AutorunStartCollision Value="AutorunCollisionStart" />
		<ChangeMode_2DtoForward Value="ChageMode_2DtoForward" />
		<ChangeMode_3Dto2D Value="ChageMode_3Dto2D" />
		<ChangeMode_3DtoForward Value="ChageMode_3DtoForward" />
		<ChangeDiveBegin Value="ChangeDiveModeStart" />
		<ChangeDiveEnd Value="ChangeDiveEndNoEvent" />
		<JumpCollision Value="EventJumpCollision" />
		<StumbleCollision Value="EventStumbleCollision" />
		<GoalRing Value="GoalCollision" />
		<!-- Navigator -->
		<NavigationCollision Value="EventNavigationCollision" />
		<NavigationCollision Value="EventNavigationCollisionPlane" />
		<FallSignalSV Value="EventNavigationCollision" Condition="NavigationType:8" />
		<FallSignalSV Value="EventNavigationCollisionPlane" Condition="NavigationType:8" />
		<Omochao Value="HintRing" />
		<!-- Camera -->
		<ObjCameraPathParallel Value="Camera2D" />
		<ObjCameraNormal Value="Camera3D" />
		<ChangeVolumeCamera Value="EventVolumeCameraChange" />
		<ObjCameraFix Value="FixCamera" />
		<ObjCameraPan Value="PanCamera" />
		<ObjCameraPanVertical Value="PanVerticalCamera" />
		<ObjCameraParallel Value="ParallelCamera" />
		<ObjCameraPathLook Value="PathLookCamera" />
		<ObjCameraPathPath Value="PathPathCamera" />
		<ObjCameraPathTarget Value="PathTargetCamera" />
		<ObjCameraPoint Value="PointCamera" />
		<ObjCameraVertical Value="PointVerticalCamera" />
		<!-- Enemies -->
		<EnemyBatabata2D Value="enmBatabata" />
		<EnemyBeeton2D Value="enmBeeton" />
		<EnemyEChaserManager Value="enmEggChaser" />
		<EnemyPawn3D Value="enmEggPawn" Condition="MoveTypeSideView:false" />
		<EnemyPawn2D Value="enmEggPawn" Condition="MoveTypeSideView:true" />
		<EnemyPawnPla3D Value="enmEggPawn_rso" />
		<EnemyPawnPla3D Value="enmEggPawn_fct" />
		<EnemyPawnPla3D Value="enmEggPawn_shp" />
		<EnemyPawnPla3D Value="enmEggPawn_pla" />
		<EnemyPawnPla3D Value="enmEggPawn_qua" />
		<EnemyPawnPla3D Value="enmEggPawn_ast" />
		<EnemyPawnPla3D Value="enmEggPawn_elv" />
		<EnemyPawnGun3D Value="enmEggPawnGun" />
		<EnemyPawnLance3D Value="enmEggPawnLance" />
		<EnemyGanigani3D Value="enmGanigani" />
		<EnemyLander3D Value="enmLander" />
		<EnemyMotora3D Value="enmMotora" />
		<EnemyNal3D Value="enmNal" />
		<EnemySpanner Value="enmSpanner" />
		<EnemySpinner Value="enmSpinner" Condition="ForSideView:false" />
		<EnemySpinner2D Value="enmSpinner" Condition="ForSideView:true" />
		<MissionSprinkler Value="enmSprinkler" />
		<CteGunTruck200 Value="enmBigChaser" />
		<!-- Wisp -->
		<PlaReleaseBox Value="ReleaseBoxSmall" />
		<SuperRing Value="ReleaseBoxSmall" Condition="PixieType:0" />
		<CpzPipeEnterCollision Value="EventSpiralPipe" />
		<GoalRing Value="ReleaseBoxBig" />
		<PlaPhantomCancelVolume Value="PhantomCancelCollision" />
		<PlaPhantomCancelVolume Value="EventLaserCancelCollision" />
		<MissionOpenBlock Value="PuzzleRing" />
		<PlaFallBlock Value="PhantomObjBreakable" />
		<!-- Sound -->
		<SoundPoint Value="EnvSound" />
		<SoundLine Value="PathSoundSource" />
		<!-- Mission -->
		<MissionChainFloor Value="ChainFloor" />
		<MissionChainThornBall Value="ChainSpikeBall" />
	</Rename>
	<MakeObjectPhysics>
		<PushBox />
		<IronBox />
		<PulleyColumn />
		<DestBox />
		<ConnectionPipe />
		<Fiber />
		<Prism />
		<CogWheel />
		<ThornCylinder />
		<Generator />
		<BattleShipShutter />
		<Partition />
		<Monolith />
		<LightRoad />
		<WarpHole />
		<ShortWarp />
		<Catapult />
		<GlassFloor />
		<AquariumDoor />
		<Moray />
		<Fountain />
		<Fishbush />
		<Meteor />
		<BreakableMeteor />
		<LongTank />
		<Pot />
		<Hatch />
		<PushBlock />
		<ChaserBrokenBlock />
	</MakeObjectPhysics>
	<RemoveObject>
		<CleanKill />
		<enmMole />
		<enmLarva />
		<DriftCollision />
	</RemoveObject>
	<PositionOffset>
		<Ring X="0" Y="0.5" Z="0" />
		<SuperRing X="0" Y="0.6" Z="0" />
		<RedMedal X="0" Y="1" Z="0" />
		<GoalRing X="0" Y="2" Z="0" />
	</PositionOffset>
	<RotationOffset>
		<DashPanel X="0" Y="-180" Z="0" Condition="IsFront:false" />
		<GrindDashPanel X="0" Y="-180" Z="0" Condition="IsFront:false" />
		<FallDeadCollision X="-90" Y="0" Z="0" />
		<DashRing X="0" Y="-180" Z="0" />
		<JumpBoard X="0" Y="-180" Z="0" />
		<JumpBoard3D X="0" Y="-180" Z="0" />
		<PointMarker X="0" Y="-180" Z="0" />
		<ChangeMode_2DtoForward X="0" Y="-180" Z="0" />
		<ChangeMode_3Dto2D X="0" Y="-180" Z="0" />
		<ChangeMode_3DtoForward X="0" Y="-180" Z="0" />
		<RainbowRing X="0" Y="-180" Z="0" />
		<AdlibTrickJump X="0" Y="-180" Z="0" />
		<CameraCollisionBoard X="0" Y="-180" Z="0" />
		<JumpCollision X="0" Y="-180" Z="0" />
		<WindNoiseCollision X="0" Y="-180" Z="0" />
		<AutorunStartCollision X="0" Y="-180" Z="0" />
		<AutorunFinishCollision X="0" Y="-180" Z="0" />
		<KeepRunningCollision X="0" Y="-180" Z="0" />
		<SetParticle X="0" Y="-90" Z="0" />
		<GrindThorn X="0" Y="-90" Z="0" /> 
		<ObjectPhysics X="-90" Y="0" Z="0" Condition="OriginalName:Fiber" />
	</RotationOffset>
	<ParamModify>
		<DashPanel>
			<IsVisibleModel Rename="IsInvisible" BoolFlip="true" />
		</DashPanel>
		<JumpBoard3D>
			<SizeType Offset="-1" />
		</JumpBoard3D>
		<JumpBoard>
			<SizeType Rename="AngleType" Offset="1" />
		</JumpBoard>
		<AdlibTrickJump>
			<FirstSpeed Rename="ImpulseSpeedOnNormal" Factor="0.1" />
			<FirstOutOfControl Rename="OutOfControl" />
			<SecondSpeed Rename="ImpulseSpeedOnBoost" Factor="0.1" />
			<IsSideView Override="0" Condition="IsSideView:true" />
			<IsSideView Override="1" Condition="IsSideView:false" />
			<IsSideView Rename="SizeType" />
		</AdlibTrickJump>
		<PointMarker>
			<StageType Rename="DimensionType" />
		</PointMarker>
		<Pulley>
			<MinSpeed Rename="MinInitVel" Factor="0.1" />
		</Pulley>
		<Spring>
			<HasBase Rename="IsWithBase" />
		</Spring>
		<SelectCanon>
			<FrontJumpForce Rename="ShotForce" Factor="0.1" />
			<FrontJumpOutOfControl Rename="OutOfControl" />
			<DownShotForce Rename="ShotForce_Fail" Factor="0.1" />
			<DownShotOutOfControl Rename="OutOfControl_Fail" />
			<FirstDirection Rename="SuccessDir" />
		</SelectCanon>
		<JumpSelector>
			<FrontJumpForce Factor="0.1" />
			<UpJumpForce Factor="0.1" />
			<DownShotForce Factor="0.1" />
			<FirstDirection Rename="SuccessButton" />
		</JumpSelector>
		<Switch>
			<TargetList Rename="TargetListON" />
			<TargetList2 Rename="TargetListOFF" />
			<EventTimer Rename="TimerON" />
			<EventType Rename="EventON" />
			<EventTimer2 Rename="TimerOFF" />
			<EventType2 Rename="EventOFF" />
			<SwitchType Rename="Type" />
		</Switch>
		<Fan>
			<Height Rename="KeepHeight" Factor="0.1" />
			<Velocity Rename="TrgHeight" Factor="0.1" />
			<OnTime Rename="Time_On" />
			<OffTime Rename="Time_Off" />
			<Place Override="1" Condition="OriginalName:WaterFan" />
		</Fan>
		<RedMedal>
			<ItemId Rename="MedalID" />
		</RedMedal>
		<DirectionalThorn>
			<UsePanel Rename="IsUsePanel" />
		</DirectionalThorn>
		<SshBubble>
			<VanishHeight Rename="LifeTime" Factor="0.1" />
		</SshBubble>
		<GeneralFloor>
			<Amplitude Factor="0.1" />
		</GeneralFloor>
		<ObjectPhysics>
			<ObjTypeID Rename="Type" EnumString="true" Condition="OriginalName:Gismo" />
			<ModelType Rename="Type" EnumString="true" Condition="OriginalName:MapPartsMesh" />
			<ModelType Rename="Type" EnumString="true" Condition="OriginalName:MapPartsBox" />
		</ObjectPhysics>
		<PlaReleaseBox>
			<PixieType Override="6" Condition="PixieType:4" />
			<PixieType Override="7" Condition="PixieType:3" />
			<PixieType Rename="Type" />
			<Revive Rename="IsRevive" />
			<MessageBoot Rename="IsMsgBoot" />
		</PlaReleaseBox>
		<CpzPipeEnterCollision>
			<CollisionDepth Rename="Collision_Length" Factor="0.2" />
		</CpzPipeEnterCollision>
		<LayerChangeCollisionFlat>
			<OnLayer0 Rename="ONLayerIndex" />
			<OnLayer1 Rename="ONLayerIndex1" />
			<OffLayer0 Rename="OFFLayerIndex" />
			<OffLayer1 Rename="OFFLayerIndex1" />
		</LayerChangeCollisionFlat>
		<AutorunStartCollision>
			<ForceToGround Override="0" Condition="ForceToGround:false" />
			<ForceToGround Override="1" Condition="ForceToGround:true" />
			<ForceToGround Rename="IsForceToGround" />
			<SendType Rename="Type" />
		</AutorunStartCollision>
		<ChangeMode_2DtoForward>
			<CurveCorrectionForce Rename="m_CurveCorrectionForce" />
			<DashPathSideMoveRate Rename="m_DashPathSideMoveRate" />
			<IsAutoChange2DPath Rename="m_IsAutoChange2DPath" />
			<IsChangeCamera Rename="m_IsChangeCamera" />
			<IsEnableFromBack Rename="m_IsEnableFromBack" />
			<IsEnableFromFront Rename="m_IsEnableFromFront" />
			<IsLimitEdge Rename="m_IsLimitEdge" />
			<IsReverseCameraEnable Rename="m_IsReverseCameraEnable" />
			<PathCorrectionForce Rename="m_PathCorrectionForce" />
			<PathEaseTime Rename="m_PathEaseTime" />
		</ChangeMode_2DtoForward>
		<ChangeMode_3DtoForward>
			<CurveCorrectionForce Rename="m_CurveCorrectionForce" />
			<DashPathSideMoveRate Rename="m_DashPathSideMoveRate" />
			<IsChangeCamera Rename="m_IsChangeCamera" />
			<IsAirKeepLine Rename="m_IsEnableAirBoost" />
			<IsEnableFromBack Rename="m_IsEnableFromBack" />
			<IsEnableFromFront Rename="m_IsEnableFromFront" />
			<IsKeepLine Rename="m_IsGravityControl" />
			<IsLimitEdge Rename="m_IsLimitEdge" />
			<IsReverseCameraEnable Rename="m_IsReverseCameraEnable" />
			<PathCorrectionForce Rename="m_PathCorrectionForce" />
		</ChangeMode_3DtoForward>
		<JumpCollision>
			<ImpulseSpeed Rename="ImpulseSpeedOnNormal" Factor="0.1" />
			<ImpulseSpeedBoost Rename="ImpulseSpeedOnBoost" Factor="0.1" />
			<SpeedMin Rename="SpeedMin" Factor="0.1" />
			<IsGroundOnly Rename="m_IsGroundOnly" />
		</JumpCollision>
		<SoundPoint>
			<CueID Rename="CueName" EnumString="true" />
			<IsPlayOnce Rename="IsRegularly" BoolFlip="true" />
			<RangeIn Rename="Range" Factor="0.1" />
			<RangeOut Rename="Radius" Factor="0.1" />
		</SoundPoint>
		<SoundLine>
			<CueID Rename="CueName" EnumString="true" />
		</SoundLine>
		<WindNoiseCollision>
			<CueID Rename="CueName" EnumString="true" />
		</WindNoiseCollision>
		<SetParticle>
			<CueID Rename="EffectName" EnumString="true" />
		</SetParticle>
		<SetRigidBody>
			<DefaultON Override="false" Condition="PhysicsType:1" />
		</SetRigidBody>
		<NavigationCollision>
			<Width Rename="Collision_Width" Factor="0.2" />
			<Height Rename="Collision_Height" Factor="0.2" />
			<DisAppearType Rename="DelayType" />
			<OnlyOneBoot Rename="OnlyOnce" />
			<EnterAngle Rename="InputAngle" />
			<NaviType Rename="NavigationType" />
			<NavigationType Override="3" Condition="Type:0" />
			<NavigationType Override="0" Condition="Type:2" />
			<NavigationType Override="2" Condition="Type:3" />
			<NavigationType Override="8" Condition="Type:5" />
			<NavigationType Override="10" Condition="Type:6" />
			<NavigationType Override="6" Condition="Type:7" />
			<NavigationType Override="3" Condition="NavigationType:0" />
			<NavigationType Override="0" Condition="NavigationType:2" />
			<NavigationType Override="2" Condition="NavigationType:3" />
			<NavigationType Override="8" Condition="NavigationType:5" />
			<NavigationType Override="10" Condition="NavigationType:6" />
			<NavigationType Override="6" Condition="NavigationType:7" />
		</NavigationCollision>
		<EventCollision>
			<CollisionDepth Rename="Collision_Length" Factor="0.2" />
			<TargetList Rename="TargetList0" />
			<TargetList2 Rename="TargetList1" />
			<TargetList3 Rename="TargetList2" />
			<EventTimer Rename="Timer0" />
			<EventType Rename="Event0" />
			<EventTimer2 Rename="Timer1" />
			<EventType2 Rename="Event1" />
			<EventTimer3 Rename="Timer2" />
			<EventType3 Rename="Event2" />
		</EventCollision>
		<EventSetter>
			<TargetList Rename="TargetList0" />
			<EventList Rename="TriggerList" />
			<EventTimer Rename="Timer0" />
			<EventNotifier Rename="Event0" />
			<EventTimer2 Rename="Timer1" />
			<EventNotifier2 Rename="Event1" />
			<EventTimer3 Rename="Timer2" />
			<EventNotifier3 Rename="Event2" />
		</EventSetter>
		<!-- Camera -->
		<CameraCollisionBoard>
			<CameraA Rename="ACameraID" />
			<CollisionA Rename="ALinkObjID" />
			<SideTypeA Rename="ALinkSide" />
			<EaseTimeA Rename="EaseTime_AtoB" />
			<PriorityA Rename="ACameraPriority" />
			<CameraB Rename="BCameraID" />
			<CollisionB Rename="BLinkObjID" />
			<SideTypeB Rename="BLinkSide" />
			<PriorityB Rename="BCameraPriority" />
			<EaseTimeB Rename="EaseTime_BtoA" />
			<RangeOut Rename="Range" Factor="0.2" />
		</CameraCollisionBoard>
		<ChangeVolumeCamera>
			<RangeOut Rename="Range" Factor="0.2" />
		</ChangeVolumeCamera>
		<ObjCameraPathParallel>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPathParallel>
		<ObjCameraNormal>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraNormal>
		<ObjCameraFix>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraFix>
		<ObjCameraPan>
			<PositionMode Rename="CameraPositionMode" />
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPan>
		<ObjCameraPanVertical>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPanVertical>
		<ObjCameraParallel>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraParallel>
		<ObjCameraPathLook>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPathLook>
		<ObjCameraPathPath>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPathPath>
		<ObjCameraPathTarget>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPathTarget>
		<ObjCameraPoint>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraPoint>
		<ObjCameraVertical>
			<RangeOut Rename="Range" Factor="1" />
		</ObjCameraVertical>
		<!-- Enemy -->
		<EnemyPawn3D>
			<RadiusAttack Rename="AttackRange" Factor="0.1" />
			<RadiusActive Rename="ActionRange" Factor="0.1" />
			<IsPlayFindMotion Rename="IsPlayFindMotion" />
		</EnemyPawn3D>
		<all>
			<!-- Parameters that match... -->
			<CollisionWidth Rename="Collision_Width" Factor="0.2" />
			<CollisionHeight Rename="Collision_Height" Factor="0.2" />
			<CollisionLength Rename="Collision_Length" Factor="0.2" />
			<CollisionDepth Rename="Collision_Depth" Factor="0.2" />
			<EaseTimeEnter Rename="Ease_Time_Enter" />
			<EaseTimeLeave Rename="Ease_Time_Leave" />
			<FovY Rename="Fovy" />
			<InterpolateType Rename="LineType" />
			<ShapeType Rename="Shape_Type" />
			<TargetType Rename="Target_Type" />
			<TargetOffsetFront Rename="TargetOffset_Front" Factor="0.1" />
			<TargetOffsetUp Rename="TargetOffset_Up" Factor="0.1" />
			<TargetOffsetRight Rename="TargetOffset_Right" Factor="0.1" />
			<TargetFrontOffsetSpeedScale Rename="TargetOffset_Vel" Factor="0.1" />
			<!-- Parameters that contain... -->
			<Collision. Factor="0.2" />
			<Speed. Factor="0.1" />
			<Velocity. Factor="0.1" />
			<!-- <Force. Factor="0.1" /> -->
			<Gravity. Factor="0.1" />
			<TargetOffset. Factor="0.1" />
			<Distance. Factor="0.1" />
			<Height. Factor="0.1" />
			<Width. Factor="0.1" />
			<Length. Factor="0.1" />
			<Radius. Factor="0.1" />
			<RangeOut. Rename="Range" Factor="0.1" />
			<RangeIn. Factor="0.1" />
		</all>
	</ParamModify>
	<IDOffset Value="0" />
	<GlobalPositionOffset X="0" Y="0" Z="0" />
</Modifiers>